<?xml version="1.0" encoding="ISO-8859-1" ?>

<expconfig>
  <experimentdef>
    <!-- Experiment configuration -->
    <source>
      <type>SeqFileIn</type>
      <name>dataLeft</name>
    </source>
    <source>
      <type>SeqFileIn</type>
      <name>dataRight</name>
    </source>
    <transform>
      <type>SeqFilter</type>
      <name>filterHi</name>
      <input>dataLeft</input>
      <configuration>
        <properties fileName="Example-Hi.properties" basePath="config"/>
      </configuration>
    </transform>
    <transform>
      <type>SeqFilter</type>
      <name>filterLo</name>
      <input>dataRight</input>
      <configuration>
        <properties fileName="Example-Lo.properties" basePath="config"/>
      </configuration>
    </transform>
    <analysis>
      <type>SeqPatternMine</type>
      <name>SPMHi</name>
      <input>filterHi</input>
      <configuration>
        <properties fileName="Example-Hi.properties" basePath="config"/>
      </configuration>
    </analysis>
    <analysis>
      <type>SeqPatternMine</type>
      <name>SPMLo</name>
      <input>filterLo</input>
      <configuration>
        <properties fileName="Example-Lo.properties" basePath="config"/>
      </configuration>
    </analysis>
    <analysis>
      <type>DiffSeqMine</type>
      <connection>
 	     <source>filterHi</source>
 	     <target>Left</target>
      </connection>
      <connection>
 	     <source>filterLo</source>
 	     <target>Right</target>
      </connection>
    </analysis>
    <analysis>
      <type>ResultFilter</type>
      <input>DiffSeqMine</input>
    </analysis>
    <analysis>
      <type>ResultSort</type>
      <input>ResultFilter</input>
    </analysis>
    <output>
      <type>OutSeqPatternResults</type>
      <input>ResultSort</input>
    </output>
    <parametergroup>
      <parameter>
        <key>dsm.minsupport</key>
        <type>RangeDbl</type>
        <begin>0.8</begin>
        <end>0.9</end>
        <step>0.05</step>
      </parameter>
      <parameter>
        <key>experiment.dsm.minsupport.name</key>
        <type>List</type>
        <value>Low</value>
        <value>Med</value>
        <value>Hi</value>
      </parameter>
    </parametergroup>
    <parameter>
      <key>dsm.maxgap</key>
      <type>RangeInt</type>
      <begin>0</begin>
      <end>1</end>
      <step>1</step>
    </parameter>
    <parameterstr>
      <key>out.seqresults.file.filename</key>
      <prefix>results_dsm</prefix>
      <postfix>.csv</postfix>
      <parameterdelimiter>--</parameterdelimiter>
      <valuedelimiter>-</valuedelimiter>
      <parameter>
        <key>dsm.minsupport</key>
        <name>SSup</name>
      </parameter>
      <parameter>
        <key>experiment.dsm.minsupport.name</key>
        <name>SSupLev</name>
      </parameter>
      <parameter>
        <key>dsm.maxgap</key>
        <name>Gap</name>
      </parameter>
    </parameterstr>
    <parameterstr>
      <key>spm.out.filename</key>
      <prefix>results_spm</prefix>
      <postfix>.csv</postfix>
      <parameterdelimiter>--</parameterdelimiter>
      <valuedelimiter>-</valuedelimiter>
      <parameter>
        <key>filter.seq.out.name</key>
        <name>Data</name>
      </parameter>
      <parameter>
        <key>spm.minsupport</key>
        <name>SSup</name>
      </parameter>
      <parameter>
        <key>experiment.dsm.minsupport.name</key>
        <name>SSupLev</name>
      </parameter>
      <parameter>
        <key>spm.maxgap</key>
        <name>Gap</name>
      </parameter>
    </parameterstr>
  </experimentdef>
</expconfig>
